<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spud.nimbus.nimbus_product.dao.SkuStockMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.spud.nimbus.nimbus_product.model.SkuStock">
        <id column="stock_id" property="stockId" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="sku_id" property="skuId" />
        <result column="actual_stock" property="actualStock" />
        <result column="lock_stock" property="lockStock" />
        <result column="stock" property="stock" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        stock_id, create_time, update_time, sku_id, actual_stock, lock_stock, stock
    </sql>

</mapper>
